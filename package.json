{
  "author": "vijayashree-at-411977029542 @vijayashreed",
  "bin": {
    "oclif-cli": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.7.0",
    "aws-sdk": "^2.1477.0",
    "bytenode": "^1.5.2",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.11",
    "ora": "^7.0.1",
    "path": "^0.12.7",
    "tsc-alias": "^1.8.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "oclif example Hello World CLI (ESM)",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.11.4",
    "@types/colors": "^1.2.1",
    "@types/fs-extra": "^11.0.2",
    "@types/inquirer": "^9.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18.18.6",
    "@types/sinon": "^10.0.20",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^3.16.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/vijayashreed/oclif-cli",
  "license": "MIT",
  "main": "",
  "name": "@juego/oclif-cli",
  "oclif": {
    "bin": "oclif-cli",
    "dirname": "oclif-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "vijayashreed/oclif-cli",
  "scripts": {
    "build": "shx rm -rf dist && rm -rf tsconfig.tsbuildinfo && tsc -b && tsc-alias -p tsconfig.json",
    "build-with-aliases": "NODE_OPTIONS='-r module-alias/register' shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only   \"test/**/*.test.ts\" ",
    "version": "oclif readme && git add README.md",
    "generate": "node ./bin/run.js generate"
  },
  "workspaces": [
    "./dist/*"
  ],
  "version": "0.0.23",
  "bugs": "https://github.com/vijayashreed/oclif-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
